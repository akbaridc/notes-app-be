{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off"
    },
    "spaced-comment": ["error", "always", {
        "line": {
            "markers": ["/"],
            "exceptions": ["-", "+"]
            
        },
        "block": {
            "markers": ["!"],
            "exceptions": ["*"],
            "balanced": true
        }
    }]
}
